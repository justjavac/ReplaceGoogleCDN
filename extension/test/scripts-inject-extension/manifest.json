{
  "name": "script inject extension",
  "version": "0.1.2",
  "manifest_version": 3,
  "description": "inject js ",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "webNavigation",
    "contentSettings"
  ],
  "incognito": "split",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*",
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "css": [
        "my-styles.css"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_security_policy": {
    "extension_pages": "default-src *;  object-src 'self' ;script-src  'self'  ; script-src-elem  'self' ;style-src 'unsafe-inline' ",
    "sandbox": "sandbox allow-scripts; script-src 'self' ;script-src-elem  'self'",
    "isolated_world": "script-src 'self'"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "content-script2.js",
        "inject.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "use_dynamic_url": true,
      "extension_ids": []
    }
  ],
  "commands":{
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      }
    }
  }
}